<%- include("includes/header") %>
<section>
    <div class="feature-photo">
        <figure>
            <img id="group-cover-photo">
        </figure>
        <div class="add-btn">
            <span id="group-members"></span>
        </div>
        <div class="container-fluid">
            <div class="row merged">
                <div class="col-md-9">
                    <div class="timeline-info"></div>
                    <ul>
                        <li class="admin-name">
                            <h5 style="padding: 25px;" id="group-name"></h5>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</section>
<section>
    <div class="gay gray-bg">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <div class="row" id="page-contents">
                        <div class="col-md-3">
                            <%-include ("includes/left-sidebar") %>
                        </div>
                        <div class="col-md-6">
                            <div class="add-post-box"></div>
                            <div class="loadMore" id="newsfeed"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<input type="hidden" id="_id" value="<%= _id%>">

